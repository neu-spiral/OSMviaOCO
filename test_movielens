#!/bin/bash

#SBATCH --nodes=1

#SBATCH --time=23:00:00

#SBATCH --job-name='FL_MovieLens'

#SBATCH --partition=short

#SBATCH --constraint=broadwell

#SBATCH --output=slurm_logs/FL_movielens_%j.out

#SBATCH --error=slurm_logs/FL_movielens_%j.err

srun python main.py --problemType FL --input datasets/MovieLens_294_users_21_movies --partitions datasets/MovieLens_294_users_21_movies_partitions --policy OMD --eta $1 --gamma $2 --k 1 --seed $3 --T 294
